@inherits LayoutComponentBase

<AuthorizeView>
	<Authorizing>
		@Body
	</Authorizing>
	<NotAuthorized>
		<style>
			body {
				background-color: #FFF;
                overflow-x:clip;
                overflow-y:clip;
			}
		</style>
		<div class="container">
			@Body
		</div>
	</NotAuthorized>
</AuthorizeView>

